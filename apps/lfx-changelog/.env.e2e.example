# E2E Test Environment
# Copy to .env.e2e and fill in values

# Test database (postgres-test service on port 5433)
DATABASE_URL=postgresql://changelog:changelog_dev@localhost:5433/lfx_changelog_test

# Auth0 test user credentials
E2E_SUPER_ADMIN_EMAIL=superadmin@example.com
E2E_SUPER_ADMIN_PASSWORD=
E2E_PRODUCT_ADMIN_EMAIL=productadmin@example.com
E2E_PRODUCT_ADMIN_PASSWORD=
E2E_EDITOR_EMAIL=editor@example.com
E2E_EDITOR_PASSWORD=
E2E_USER_EMAIL=user@example.com
E2E_USER_PASSWORD=

# Auth0 user IDs (sub claims) â€” get these from Auth0 after creating users
E2E_SUPER_ADMIN_AUTH0_ID=auth0|REPLACE_WITH_ACTUAL_ID
E2E_PRODUCT_ADMIN_AUTH0_ID=auth0|REPLACE_WITH_ACTUAL_ID
E2E_EDITOR_AUTH0_ID=auth0|REPLACE_WITH_ACTUAL_ID
E2E_USER_AUTH0_ID=auth0|REPLACE_WITH_ACTUAL_ID
